<h1>Prodi</h1>

    <!-- Tabel Prodi -->
    <table border="1">
        <thead>
            <tr>
                <th>Nama</th>
                <th>Singkatan</th>
                <th>Fakultas</th>
            </tr>
        </thead>
        <tbody>
            <% prodi.forEach(item => { %>
                <tr>
                    <td><%= item.nama %></td>
                    <td><%= item.singkatan %></td>
                    <td><%= item.fakultas %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>